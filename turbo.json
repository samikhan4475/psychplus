{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": ["**/.env", "tsconfig.json"],
  "pipeline": {
    "build": {
      "cache": true,
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "dev": {
      "cache": false
    },
    "lint": {
      "cache": true
    },
    "lint:fix": {
      "cache": false
    },
    "clean": {
      "cache": false
    }
  },
  "globalEnv": [
    "NODE_ENV",
    "APP_CODE",
    "APP_PATH",
    "DISABLE_APP_PATH",
    "APP_VERSION",
    "APP_ENV",
    "CLINICAL_URL",
    "PATIENT_URL",
    "REVCYCLE_URL",
    "PLATFORM_URL",
    "API_URL",
    "AUTH_API_URL",
    "NEXT_PUBLIC_MAPBOX_ACCESS_TOKEN",
    "NEXT_PUBLIC_RECAPTCHA_V2_KEY",
    "NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY",
    "NEXT_PUBLIC_GOOGLE_MAPS_API_KEY",
    "NEXT_RUNTIME",
    "ROOT_DOMAIN",
    "SCRIPTSURE_PATIENT_PORTAL_USER_EMAIL",
    "SCRIPTSURE_API_KEY",
    "SCRIPTSURE_API_SECRET",
    "SCRIPTSURE_BASE_PLATFORM_URL",
    "SCRIPTSURE_BASE_APPLICATION_URL",
    "SCRIPTSURE_ENCRYPTION_KEY",
    "GOOGLE_MAPS_API_KEY",
    "AUTH_USER_COOKIE_NAME",
    "AUTH_ACCESS_TOKEN_COOKIE_NAME",
    "AUTH_ACCESS_TOKEN_EXPIRY_COOKIE_NAME",
    "AUTH_REFRESH_TOKEN_COOKIE_NAME",
    "SCRIPTSURE_SESSION_COOKIE_NAME",
    "SCRIPTSURE_SESSION_COOKIE_CREATED_AT",
    "STRIPE_API_KEY",
    "STRIPE_PUBLISHABLE_KEY",
    "AUTH_URL",
    "SCRIPTSURE_SECRET",
    "SCRIPTSURE_PLATFORM_URL",
    "SCRIPTSURE_APP_URL",
    "USER_COOKIE",
    "ACCESS_TOKEN_COOKIE",
    "ACCESS_TOKEN_EXPIRY_COOKIE",
    "REFRESH_TOKEN_COOKIE",
    "SCRIPTSURE_SESSION_COOKIE",
    "SCRIPTSURE_SESSION_CREATED_AT_COOKIE",
    "SESSION_ID_COOKIE",
    "PRACTICE_ID_COOKIE",
    "WEBSOCKETSERVICE_URL"
  ]
}
